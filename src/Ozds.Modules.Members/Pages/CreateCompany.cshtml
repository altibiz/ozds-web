@page
@model Ozds.Modules.Members.Pages.CreateCompanyModel
@using OrchardCore.ContentManagement.Metadata

@inject IContentDefinitionManager ContentDefinitionManager


@{ Html.RenderPartial("_Header", T["Add Legal Entity"].Value); }


@await RenderSectionAsync("Messages", required: false)

<h5>2. @T["Add Legal Entity Details"]</h5>
<form method="post" enctype="multipart/form-data">

    <div asp-validation-summary="All" class="text-danger"></div>

    @await DisplayAsync(Model.Shape)

    <button type="submit" class="btn btn-success publish">@T["Submit"]</button>

</form>