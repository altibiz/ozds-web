{
  "name": "ozds-web",
  "private": true,
  "scripts": {
    "build": "yarn workspace ozds-themes-ozds-assets build && yarn gulp build",
    "watch": "yarn wait-on http://localhost:3000 && yarn build && yarn gulp watch",
    "start": "yarn concurrently -n ozds-themes-ozds-assets,gulp-watch -c blue.bold,red.bold \"yarn workspace ozds-themes-ozds-assets start\" \"yarn watch\"",
    "debug": "yarn concurrently -n ozds-themes-ozds-assets,gulp-watch -c blue.bold,red.bold \"yarn workspace ozds-themes-ozds-assets debug\" \"yarn watch\"",
    "format": "yarn prettier --write --ignore-path .gitignore ./**/*.{js,ts,css,scss,graphql,yml,json,md,pug}",
    "lint": ""
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-flow": "7.14.5",
    "@prettier/plugin-pug": "^1.20.1",
    "all-contributors-cli": "^6.20.0",
    "concurrently": "6.2.0",
    "es6-promise": "4.2.8",
    "glob": "^7.1.7",
    "graceful-fs": "4.2.6",
    "gulp": "^4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "2.6.1",
    "gulp-dart-sass": "1.0.2",
    "gulp-debug": "4.0.0",
    "gulp-eol": "0.2.0",
    "gulp-header": "2.0.9",
    "gulp-if": "3.0.0",
    "gulp-less": "5.0.0",
    "gulp-minifier": "^3.5.0",
    "gulp-newer": "1.4.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-print": "5.0.2",
    "gulp-rename": "2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "2.0.1",
    "gulp-typescript": "5.0.1",
    "gulp-util": "3.0.8",
    "husky": "^7.0.4",
    "merge-stream": "2.0.0",
    "npm-run-all": "^4.1.5",
    "path-posix": "1.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "typescript": "^4.3.4",
    "upath": "^2.0.1",
    "wait-on": "^5.3.0"
  },
  "packageManager": "yarn@3.2.0",
  "workspaces": [
    "src/Ozds.Themes.Ozds.Assets"
  ],
  "dependenciesMeta": {
    "bootstrap@5.0.1": {
      "unplugged": true
    }
  }
}
